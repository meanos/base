#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'cloud-init==19.1','console_scripts','cloud-id'
__requires__ = 'cloud-init==19.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('cloud-init==19.1', 'console_scripts', 'cloud-id')()
    )
